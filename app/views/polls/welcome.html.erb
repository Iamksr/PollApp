<p id="notice"><%= notice %></p>

<h1>Polls</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Desciption</th>
      <th colspan="3"></th>
    </tr>
  </thead>
<div class = "container">
  <tbody>
    <% @polls.each do |poll| %>
      <tr>
        <td><%= poll.title %></td>
        <td><%= poll.description %></td>
        <td><%= link_to 'Show', poll,class: "btn btn-primary" %></td>
      </tr>
    <% end %>
  </tbody>
  </div>
</table>

<br>
